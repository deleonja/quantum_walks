Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Routine", " ", "to", " ", "compute", " ", "the", " ", "superoperator", " ",
     "of", " ", 
    RowBox[{"coin", "'"}], "s", " ", "quantum", " ", "channel", " ", "at", " ",
     "time", " ", "t"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ClearAll", "[", "Superoperator", "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Superoperator", "[", "t_", "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Psi]fCP", ",", "\[Rho]fCP"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Initiallize", " ", "DTQW", " ", "routine"}], "  ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"InitializeDTQW", "[", 
        RowBox[{"2", ",", 
         RowBox[{
          RowBox[{"2", "t"}], "+", "1"}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"MakeCoin", "[", 
        RowBox[{
         RowBox[{"1", "/", "2."}], ",", "0", ",", "0"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"MakeShift", "[", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"MakeUnitary", "[", "]"}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Final", " ", "states", " ", "of", " ", "Coin"}], " ", "+",
            " ", 
           RowBox[{"Position", " ", 
            RowBox[{"(", "CP", ")"}], " ", "for", " ", "two", " ", 
            RowBox[{"DTQW", ":", " ", 
             RowBox[{
              RowBox[{"(", "1", ")"}], " ", 
              TemplateBox[{
                SubscriptBox["\[Psi]", "0"]},
               "Ket"]}]}]}]}], "=", 
          RowBox[{
           SubscriptBox[
            TemplateBox[{"0"},
             "Ket"], "c"], "\[CircleTimes]", 
           SubscriptBox[
            TemplateBox[{"0"},
             "Ket"], "p"]}]}], ",", " ", 
         RowBox[{
          RowBox[{"and", " ", 
           RowBox[{"(", "2", ")"}], " ", 
           TemplateBox[{
             SubscriptBox["\[Psi]", "0"]},
            "Ket"]}], "=", 
          RowBox[{
           SubscriptBox[
            TemplateBox[{"1"},
             "Ket"], "c"], "\[CircleTimes]", 
           SubscriptBox[
            TemplateBox[{"0"},
             "Ket"], "p"]}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[Psi]fCP", "=", 
        RowBox[{"Flatten", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"DTQW", "[", 
            RowBox[{
             RowBox[{"VectorState", "[", 
              RowBox[{"{", 
               RowBox[{"{", 
                RowBox[{"1.", ",", "0", ",", "0"}], "}"}], "}"}], "]"}], ",", 
             "t"}], "]"}], ",", 
           RowBox[{"DTQW", "[", 
            RowBox[{
             RowBox[{"VectorState", "[", 
              RowBox[{"{", 
               RowBox[{"{", 
                RowBox[{"1.", ",", "1", ",", "0"}], "}"}], "}"}], "]"}], ",", 
             "t"}], "]"}]}], "}"}]}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "All", " ", "possible", " ", "outer", " ", "products", " ", "of", " ",
          "final", " ", "states", " ", "in", " ", "\[Psi]fCP"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"\[Rho]fCP", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Dyad", "[", 
           RowBox[{
            RowBox[{"\[Psi]fCP", "[", 
             RowBox[{"[", "#1", "]"}], "]"}], ",", 
            RowBox[{"\[Psi]fCP", "[", 
             RowBox[{"[", "#2", "]"}], "]"}]}], "]"}], "&"}], "@@@", 
         RowBox[{"Tuples", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "2"}], "}"}], ",", "2"}], "]"}]}]}], ";", 
